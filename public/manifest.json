{
  "manifest_version": 3,
  "name": "Linkt",
  "version": "1.0.0",
  "icons": {
    "16": "16px.png",
    "32": "32px.png",
    "48": "48px.png",
    "128": "128px.png"
  },
  "description": "Store commonly sent links in one place to save you the trouble of looking them up!",
  "action": {
    "default_popup": "index.html",
    "default_title": "Find your links now!"
  },
  "host_permissions": [
    "https://*/",
    "http://*/"
  ]
}